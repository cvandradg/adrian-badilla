{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@adrianbadilla/app": ["libs/adrianbadilla/frontend/ui/app/src/index.ts"],
      "@adrianbadilla/shared": [
        "libs/adrianbadilla/frontend/ui/shared/src/index.ts"
      ],
      "@appStore/*": ["libs/adrianbadilla/frontend/ui/app/src/lib/+state/*"],
      "@authHelper": [
        "libs/adrianbadilla/frontend/ui/app/src/lib/classes/firebaseAuthHelper.ts"
      ],
      "@classes/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/classes/*"
      ],
      "@decorators/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/decorators/*"
      ],
      "@dialogs/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/dialogs/*"
      ],
      "@dietas-adrian-nx-workspace/adrianbadilla/frontend/ui/dietas": [
        "libs/adrianbadilla/frontend/ui/dietas/src/index.ts"
      ],
      "@dietas-adrian-nx-workspace/adrianbadilla/frontend/ui/login": [
        "libs/adrianbadilla/frontend/ui/login/src/index.ts"
      ],
      "@adrianbadilla/frontend/app": [
        "libs/adrianbadilla/frontend/ui/app/src/index.ts"
      ],
      "@environments/*": [
        "libs/adrianbadilla/frontend/ui/shared/src/lib/environments/*"
      ],
      "@guards/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/guards/*"
      ],
      "@helperFunctionsService": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/services/helperFunctions.service.ts"
      ],
      "@interfaces/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/interfaces/*"
      ],
      "@libs/client/*": ["libs/adrianbadilla/frontend/ui/client/src/lib/*"],
      "@libs/dashboard": [
        "libs/adrianbadilla/frontend/ui/dashboard/src/lib/lib.routes.ts"
      ],
      "@libs/dietas": [
        "libs/adrianbadilla/frontend/ui/dietas/src/lib/lib.routes.ts"
      ],
      "@libs/dietas/*": ["libs/adrianbadilla/frontend/ui/dietas/src/lib/*"],
      "@libs/landing-page/*": ["libs/adrianbadilla/frontend/ui/login/src/lib/*"],
      "@libs/login": ["libs/adrianbadilla/frontend/ui/login/src/lib/routes.ts"],
      "@materialFunctions": [
        "libs/adrianbadilla/frontend/shared/materialFunctions.ts"
      ],
      "@services/*": [
        "libs/adrianbadilla/frontend/ui/shared/src/lib/services/*"
      ],
      "@shared-modules": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/index.ts"
      ],
      "@shared-modules/*": [
        "libs/adrianbadilla/frontend/shared/shared-module/src/lib/*"
      ],
      "client": ["libs/adrianbadilla/frontend/ui/client/src/index.ts"],
      "dashboard": ["libs/adrianbadilla/frontend/ui/dashboard/src/index.ts"],
      "landing-page": [
        "libs/adrianbadilla/frontend/ui/landing-page/src/index.ts"
      ],
      "@types": [
        "libs/adrianbadilla/frontend/ui/shared/src/lib/types/types.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
